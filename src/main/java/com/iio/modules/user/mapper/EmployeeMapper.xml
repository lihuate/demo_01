<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.iio.modules.user.mapper.EmployeeMapper">
  <select id="findAll" resultType="com.iio.modules.user.vo.Employee">
  	SELECT employee_id employeeId,concat(first_name,'.',last_name) name,email FROM employees
  </select>
</mapper>
